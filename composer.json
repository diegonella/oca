{
    "name": "diegonella/oca",
    "description": "Port para utilizar los metodos de tarifar con OCA",
    "license": "MIT",
    "authors": [
        {
            "name": "Diego Gonella",
            "email": "diegonella@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable" : true,
    "require": {
        "php" : ">=5.6.4"
    },
    "require-dev": {
        "phpunit/phpunit" : "~5.0"
    },
    "autoload": {
        "psr-4": {
            "diegonella\\oca\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "diegonella\\oca\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "diegonella\\oca\\OcaEnviosServiceProvider"
            ]
        }
    }
} 